<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.TablePrivilegesEntity" table="TABLE_PRIVILEGES" schema="information_schema">
        <property name="grantee" column="GRANTEE"/>
        <property name="tableCatalog" column="TABLE_CATALOG"/>
        <property name="tableSchema" column="TABLE_SCHEMA"/>
        <property name="tableName" column="TABLE_NAME"/>
        <property name="privilegeType" column="PRIVILEGE_TYPE"/>
        <property name="isGrantable" column="IS_GRANTABLE"/>
    </class>
</hibernate-mapping>